<h1 class="cat">clients:</h1>

<div class="row">
      <div class="col-sm">
      <table class="table">
        <thead>
          <tr>
            <th scope="col"><h2 class="header">Client Name</h2></th>
            <th scope="col"><h2 class="header">Update</h2></th>
            <th scope="col"><h2 class="header">Delete</h2></th>
            <th scope="col"><h2 class="header">More...</h2></th>
          </tr>
        </thead>
        <tbody>
          #foreach($client in $stylists.getClient())
