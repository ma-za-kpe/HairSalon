            <td class="header">$client.getName()</td>
            <td><a href="/clientsdit/$client.getId()"><button type="submit" class="btn add btn-lg "><p class="add-btn">Edit</p></button></a></td>
            <td>
            <form action="/clients/$client.getId()/delete" method="post">

            <a href="/"><button type="submit" class="btn delete btn-lg">
            <p class="add-btn">Delete</p></button>
            </a>            </form>
            </td>
          </tr>
          #end
        </tbody>
      </table>
      </div>
    </div>
</div>


